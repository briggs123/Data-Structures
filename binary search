import array as arr 
a=arr.array("i",eval(input("enter the array")))
f=int(input("enter the number you want to find"))
low=0
high=len(a)-1
b=0
c=0
while(low<=high):
    mid=(low+high)//2
    b=b+1
    if f==a[mid]:
        print("number found",a[mid])
        break
    elif f<a[mid]:
        high=mid-1
    elif f>a[mid]:
        low=mid+1
    else:
        c=c+1
