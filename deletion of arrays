#include<stdio.h>
int main()
{
    int a[20],n,pos,i,f;
    printf("enter the size of the array");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("enter the position you want to delete a number");
    scanf("%d",&pos);
    for(i=pos-1;i<=n-1;i++){
        a[i]=a[i+1];
    }
    for(i=0;i<n-1;i++){
        printf("%d",a[i]);
    }
    return 0;
}
