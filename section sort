#include<stdio.h>
int main()
{
    int a[10],n,i,j,min,temp;
    printf("Enter the size of array:");   // 6
    scanf("%d",&n);
    printf("Enter the Elements into an array:");  //2 1 3 6 5 4
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    min=0;
    for(i=min+1;i<n;i++){
        if(a[min]>a[i]){
            temp=a[min];
            a[min]=a[i];
            a[i]=temp;
            min=min+1;
        }
    };
    for(i=0;i<n;i++){
        printf("%d",a[i]);
    };
    return 0;
};
