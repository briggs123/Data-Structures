#include<stdio.h>
int quick(a){
    int pivot,low=0,high,flag==0;
    pivot=a[0];
    i=low;
    j=high-1;
    while(flag==0){
        i=i+1;
        while(a[i]<a[pivot]){
            i=i+1;
        }
        while(a[j]<a[pivot]){
            j=j+1;
        }
    }
}
int main()
{
    int a[20],i,n;
    printf("enter the size of he array");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i])
    }
    quick(a)
}
