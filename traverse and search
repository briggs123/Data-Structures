#include<stdio.h>
int main()
{
    int a[10],n,i,f;
    printf("enter the size of the array");
    scanf("%d",&n);
    printf("enter the number you want to search");
    scanf("%d",&f);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        printf("\n%d\n%p",a[i],&a[i]);
    }
    for(i=0;i<n;i++){
        if(a[i]==f){
            printf("number found");
            break;
        };
    }
    return 0;
}
