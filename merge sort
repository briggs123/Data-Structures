#include<stdio.h>
int merge_sort(int start,int end){
    int mid;
    if(start<end){
        mid=(start+end)/2;
        merge_sort(start,mid);
        merge_sort(mid+1,end);
        merging(start,mid,end);
    
}
}
int merging(int low,int mid,int high){
    int k=0;
    while(low<=mid&&high>=mid){
        if(a[low],a[high]){
            temp[k]=a[i];
            k=k+1;
            low+1;
    }
       else if(a[high]>a[low]){
           temp[k]=a[high];
           k=k+1;
           high=high+1;
       }  
}
int main()
{
    int a[20],i,n,j;
    printf("enter the size of the array");
    scanf("%d",&n);
    printf("enter the elements of the array");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    merge_sort(a,0,n-1);
    printf("%d",merging)
}
