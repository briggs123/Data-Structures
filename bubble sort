import array as arr 
def binary(a):
    n=len(a)
    for i in range(0,n-1):
        for j in range(0,n-1):
            if a[j]>a[j+1]:
                temp=a[j]
                a[j]=a[j+1]
                a[j+1]=temp
                
a=arr.array("i",eval(input("enter the array")))
binary(a)
print(a)
